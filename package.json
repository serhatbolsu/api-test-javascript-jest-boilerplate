{
  "name": "api_sample_javascript_test_boilerplate",
  "version": "1.0.0",
  "description": "Api Test of Restful services boilerplate project for Javascript with Jest",
  "main": "index.js",
  "scripts": {
    "clean": "shx rm -rf ./testResults.json ./allure-results",
    "start": "node server/server.js",
    "test": "jest ./test/ --json --outputFile='./testResults.json'",
    "test.flightapp": "jest ./test-flightapp/ --json --outputFile='./testResults.json'",
    "lint": "eslint .",
    "report": "allure serve",
    "report.teams": "node ./utils/teamsReporter.js ../testResults.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://git.emirates.com/scm/techqa/api_sit_test_framework_javascript.git"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "author": "Serhat Bolsu",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-jest": "^26.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jest": "^23.9.0",
    "husky": "4.3.0",
    "jest": "^25.4.0",
    "jest-allure": "^0.1.1",
    "shx": "^0.3.2",
    "superagent": "^5.2.2",
    "superagent-defaults": "^0.1.14",
    "superagent-logger": "^1.1.0",
    "superagent-logger": "^1.1.0"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "sequelize": "^5.21.6",
    "sqlite3": "^4.2.0"
  }
}
